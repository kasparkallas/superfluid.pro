{
	"name": "@sfpro/cms",
	"version": "1.0.0",
	"description": "A blank template to get started with Payload 3.0",
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"migrate": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" payload migrate",
		"build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
		"dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
		"devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"generate": "pnpm generate:importmap && pnpm generate:types && pnpm generate:subgraph",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"generate:subgraph": "graphclient build --dir ./src/subgraph-protocol/",
		"lint": "cross-env NODE_OPTIONS=--no-deprecation biome check .",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start",
		"test": "pnpm run test:int && pnpm run test:e2e",
		"test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
		"test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
		"trigger:dev": "pnpm dlx trigger.dev@latest dev",
		"trigger:login": "pnpm dlx trigger.dev@latest login",
		"trigger:deploy": "pnpm dlx trigger.dev@latest deploy",
		"typecheck": "tsc --noEmit",
		"cleanup:duplicate-tags": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env src/scripts/cleanup-duplicate-tags.ts"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "^7.3.4",
		"@coingecko/coingecko-typescript": "^1.13.0",
		"@libsql/client": "^0.15.15",
		"@payloadcms/db-vercel-postgres": "^3.56.0",
		"@payloadcms/email-nodemailer": "^3.56.0",
		"@payloadcms/next": "^3.56.0",
		"@payloadcms/payload-cloud": "^3.56.0",
		"@payloadcms/richtext-lexical": "^3.56.0",
		"@payloadcms/storage-vercel-blob": "^3.56.0",
		"@payloadcms/ui": "^3.56.0",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-hover-card": "^1.1.15",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.2.3",
		"@scalar/api-reference-react": "^0.7.49",
		"@superfluid-finance/metadata": "^1.6.1",
		"@superfluid-finance/tokenlist": "^5.31.0",
		"@tailwindcss/postcss": "^4.1.13",
		"@tanstack/react-query": "^5.89.0",
		"@tanstack/react-query-devtools": "^5.89.0",
		"@trigger.dev/sdk": "^4.0.2",
		"@types/lodash-es": "^4.17.12",
		"@uniswap/token-lists": "1.0.0-beta.34",
		"@vercel/blob": "^1.1.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cross-env": "^10.0.0",
		"dotenv": "^17.2.2",
		"graphql": "^16.11.0",
		"libsql": "^0.5.22",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.540.0",
		"next": "^15.5.3",
		"next-themes": "^0.4.6",
		"nuqs": "^2.6.0",
		"payload": "^3.56.0",
		"postcss": "^8.5.6",
		"react": "^19.1.1",
		"sharp": "^0.34.4",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.13",
		"viem": "^2.37.6",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@graphprotocol/client-cli": "^3.0.7",
		"@payloadcms/db-sqlite": "^3.56.0",
		"@playwright/test": "^1.55.0",
		"@testing-library/react": "^16.3.0",
		"@trigger.dev/build": "^4.0.2",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.3",
		"jsdom": "^26.1.0",
		"playwright": "^1.55.0",
		"playwright-core": "^1.55.0",
		"tsx": "^4.20.5",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.9.2",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"optionalDependencies": {
		"@libsql/darwin-arm64": "^0.5.22",
		"@libsql/linux-arm64-gnu": "^0.5.22",
		"@libsql/linux-x64-gnu": "^0.5.22"
	},
	"engines": {
		"node": "^18.20.2 || >=20.9.0",
		"pnpm": "^9 || ^10"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"sharp",
			"esbuild",
			"unrs-resolver"
		]
	}
}
