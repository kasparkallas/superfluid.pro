{
	"openapi": "3.1.0",
	"info": {
		"title": "Superfluid CMS API",
		"version": "1.0.0",
		"description": "API for accessing Superfluid token data from the CMS database, including token metadata, filtering capabilities, and optional pricing data from Redis/CoinGecko. The API also provides export functionality in Uniswap Token List format with Superfluid-specific extensions.",
		"contact": {
			"name": "Superfluid",
			"url": "https://superfluid.finance"
		}
	},
	"servers": [
		{
			"url": "",
			"description": "Current server"
		}
	],
	"tags": [
		{
			"name": "Tokens",
			"description": "Token data and metadata operations"
		},
		{
			"name": "Token List",
			"description": "Export tokens in standard formats"
		}
	],
	"components": {
		"schemas": {
			"Token": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string",
						"description": "Composite ID (chainId:address)",
						"example": "1:0x912ce59144191c1204e64559fe8253a0e49e6548"
					},
					"chainId": {
						"type": "integer",
						"exclusiveMinimum": 0,
						"description": "Blockchain network ID",
						"example": 42161
					},
					"address": {
						"type": "string",
						"description": "Ethereum contract address",
						"example": "0xb3edb2f90fec1bf1f872a9ef143cfd614773ad04"
					},
					"name": {
						"type": "string",
						"description": "Token name",
						"example": "Super ARB"
					},
					"symbol": {
						"type": "string",
						"description": "Token symbol",
						"example": "ARBx"
					},
					"decimals": {
						"type": "integer",
						"minimum": 0,
						"maximum": 18,
						"description": "Number of token decimals",
						"example": 18
					},
					"logoUri": {
						"type": ["string", "null"],
						"description": "URL of the token logo image",
						"example": "https://example.com/token-logo.png"
					},
					"isListed": {
						"type": ["boolean", "null"],
						"description": "Whether token is listed on the Resolver"
					},
					"coingeckoId": {
						"type": ["string", "null"],
						"description": "CoinGecko ID for price tracking",
						"example": "arbitrum"
					},
					"tags": {
						"type": ["array", "null"],
						"items": {
							"type": "string",
							"enum": ["streme", "testnet", "underlying", "supertoken"],
							"description": "Token categorization tags",
							"example": "supertoken"
						},
						"description": "Token categorization tags"
					},
					"tokenType": {
						"type": "string",
						"enum": ["underlyingToken", "pureSuperToken", "nativeAssetSuperToken", "wrapperSuperToken"],
						"description": "Type of token in the Superfluid ecosystem",
						"example": "wrapperSuperToken"
					},
					"underlyingAddress": {
						"type": ["string", "null"],
						"description": "Address of underlying token for wrapper super tokens",
						"example": "0xb3edb2f90fec1bf1f872a9ef143cfd614773ad04"
					},
					"note": {
						"type": ["string", "null"],
						"description": "Additional notes about the token"
					},
					"pricing": {
						"type": "object",
						"properties": {
							"usd": {
								"type": ["number", "null"],
								"description": "Current price in USD",
								"example": 0.999
							},
							"usd_24h_change": {
								"type": ["number", "null"],
								"description": "24-hour price change percentage",
								"example": -0.12
							},
							"last_updated_at": {
								"type": ["number", "null"],
								"description": "Unix timestamp of last price update",
								"example": 1736180472
							}
						},
						"required": ["usd", "usd_24h_change", "last_updated_at"],
						"description": "Current pricing data (when includePricing=true)",
						"title": "TokenPrice"
					}
				},
				"required": ["id", "chainId", "address", "name", "symbol", "decimals", "tokenType"],
				"description": "A token in the Superfluid ecosystem with metadata and optional pricing",
				"title": "Token"
			},
			"TokenListResponse": {
				"type": "object",
				"properties": {
					"tokens": {
						"type": "array",
						"items": {
							"type": "object",
							"properties": {
								"id": {
									"type": "string",
									"description": "Composite ID (chainId:address)",
									"example": "1:0x912ce59144191c1204e64559fe8253a0e49e6548"
								},
								"chainId": {
									"type": "integer",
									"exclusiveMinimum": 0,
									"description": "Blockchain network ID",
									"example": 42161
								},
								"address": {
									"type": "string",
									"description": "Ethereum contract address",
									"example": "0xb3edb2f90fec1bf1f872a9ef143cfd614773ad04"
								},
								"name": {
									"type": "string",
									"description": "Token name",
									"example": "Super ARB"
								},
								"symbol": {
									"type": "string",
									"description": "Token symbol",
									"example": "ARBx"
								},
								"decimals": {
									"type": "integer",
									"minimum": 0,
									"maximum": 18,
									"description": "Number of token decimals",
									"example": 18
								},
								"logoUri": {
									"type": ["string", "null"],
									"description": "URL of the token logo image",
									"example": "https://example.com/token-logo.png"
								},
								"isListed": {
									"type": ["boolean", "null"],
									"description": "Whether token is listed on the Resolver"
								},
								"coingeckoId": {
									"type": ["string", "null"],
									"description": "CoinGecko ID for price tracking",
									"example": "arbitrum"
								},
								"tags": {
									"type": ["array", "null"],
									"items": {
										"type": "string",
										"enum": ["streme", "testnet", "underlying", "supertoken"],
										"description": "Token categorization tags",
										"example": "supertoken"
									},
									"description": "Token categorization tags"
								},
								"tokenType": {
									"type": "string",
									"enum": ["underlyingToken", "pureSuperToken", "nativeAssetSuperToken", "wrapperSuperToken"],
									"description": "Type of token in the Superfluid ecosystem",
									"example": "wrapperSuperToken"
								},
								"underlyingAddress": {
									"type": ["string", "null"],
									"description": "Address of underlying token for wrapper super tokens",
									"example": "0xb3edb2f90fec1bf1f872a9ef143cfd614773ad04"
								},
								"note": {
									"type": ["string", "null"],
									"description": "Additional notes about the token"
								},
								"pricing": {
									"type": "object",
									"properties": {
										"usd": {
											"type": ["number", "null"],
											"description": "Current price in USD",
											"example": 0.999
										},
										"usd_24h_change": {
											"type": ["number", "null"],
											"description": "24-hour price change percentage",
											"example": -0.12
										},
										"last_updated_at": {
											"type": ["number", "null"],
											"description": "Unix timestamp of last price update",
											"example": 1736180472
										}
									},
									"required": ["usd", "usd_24h_change", "last_updated_at"],
									"description": "Current pricing data (when includePricing=true)",
									"title": "TokenPrice"
								}
							},
							"required": ["id", "chainId", "address", "name", "symbol", "decimals", "tokenType"],
							"description": "A token in the Superfluid ecosystem with metadata and optional pricing",
							"title": "Token"
						}
					},
					"pagination": {
						"type": "object",
						"properties": {
							"page": {
								"type": "integer",
								"exclusiveMinimum": 0,
								"description": "Current page number",
								"example": 1
							},
							"limit": {
								"type": "integer",
								"exclusiveMinimum": 0,
								"description": "Items per page",
								"example": 100
							},
							"totalPages": {
								"type": "integer",
								"description": "Total number of pages",
								"example": 5
							},
							"totalItems": {
								"type": "integer",
								"description": "Total number of items",
								"example": 450
							}
						},
						"required": ["page", "limit", "totalPages", "totalItems"]
					}
				},
				"required": ["tokens", "pagination"],
				"description": "Paginated list of tokens",
				"title": "TokenListResponse"
			},
			"TokenList": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string",
						"description": "Name of the token list",
						"example": "Superfluid Token List"
					},
					"timestamp": {
						"type": "string",
						"format": "date-time",
						"description": "ISO 8601 timestamp of list generation",
						"example": "2025-01-07T12:00:00.000Z"
					},
					"version": {
						"type": "object",
						"properties": {
							"major": {
								"type": "integer"
							},
							"minor": {
								"type": "integer"
							},
							"patch": {
								"type": "integer"
							}
						},
						"required": ["major", "minor", "patch"]
					},
					"tokens": {
						"type": "array",
						"items": {
							"anyOf": [
								{
									"type": "object",
									"properties": {
										"chainId": {
											"type": "integer",
											"exclusiveMinimum": 0
										},
										"address": {
											"type": "string",
											"pattern": "^0x[a-fA-F0-9]{40}$"
										},
										"name": {
											"type": "string"
										},
										"symbol": {
											"type": "string"
										},
										"decimals": {
											"type": "integer",
											"minimum": 0,
											"maximum": 18
										},
										"logoURI": {
											"type": "string"
										},
										"tags": {
											"type": "array",
											"items": {
												"type": "string"
											}
										}
									},
									"required": ["chainId", "address", "name", "symbol", "decimals"],
									"description": "Standard token information for token list",
									"title": "TokenInfo"
								},
								{
									"type": "object",
									"properties": {
										"chainId": {
											"type": "integer",
											"exclusiveMinimum": 0
										},
										"address": {
											"type": "string",
											"pattern": "^0x[a-fA-F0-9]{40}$"
										},
										"name": {
											"type": "string"
										},
										"symbol": {
											"type": "string"
										},
										"decimals": {
											"type": "integer",
											"minimum": 0,
											"maximum": 18
										},
										"logoURI": {
											"type": "string"
										},
										"tags": {
											"type": "array",
											"items": {
												"type": "string"
											}
										},
										"extensions": {
											"type": "object",
											"properties": {
												"superTokenInfo": {
													"anyOf": [
														{
															"type": "object",
															"properties": {
																"type": {
																	"type": "string",
																	"enum": ["Pure"]
																}
															},
															"required": ["type"]
														},
														{
															"type": "object",
															"properties": {
																"type": {
																	"type": "string",
																	"enum": ["Native Asset"]
																}
															},
															"required": ["type"]
														},
														{
															"type": "object",
															"properties": {
																"type": {
																	"type": "string",
																	"enum": ["Wrapper"]
																},
																"underlyingTokenAddress": {
																	"type": "string",
																	"pattern": "^0x[a-fA-F0-9]{40}$"
																}
															},
															"required": ["type", "underlyingTokenAddress"]
														}
													],
													"description": "Superfluid-specific token extension information"
												}
											},
											"required": ["superTokenInfo"]
										}
									},
									"required": ["chainId", "address", "name", "symbol", "decimals", "extensions"],
									"description": "Token information with Superfluid extensions",
									"title": "SuperTokenInfo"
								}
							]
						}
					}
				},
				"required": ["name", "timestamp", "version", "tokens"],
				"description": "Uniswap Token List format with Superfluid extensions",
				"title": "TokenList"
			},
			"TokenPrice": {
				"type": "object",
				"properties": {
					"usd": {
						"type": ["number", "null"],
						"description": "Current price in USD",
						"example": 0.999
					},
					"usd_24h_change": {
						"type": ["number", "null"],
						"description": "24-hour price change percentage",
						"example": -0.12
					},
					"last_updated_at": {
						"type": ["number", "null"],
						"description": "Unix timestamp of last price update",
						"example": 1736180472
					}
				},
				"required": ["usd", "usd_24h_change", "last_updated_at"],
				"description": "Current token price information from CoinGecko",
				"title": "TokenPrice"
			},
			"ApiError": {
				"type": "object",
				"properties": {
					"error": {
						"type": "string",
						"description": "Error type",
						"example": "Not Found"
					},
					"message": {
						"type": "string",
						"description": "Detailed error message",
						"example": "Token not found"
					}
				},
				"required": ["error", "message"],
				"description": "API error response",
				"title": "ApiError"
			}
		},
		"parameters": {}
	},
	"paths": {
		"/tokens": {
			"get": {
				"summary": "Get tokens with filtering",
				"description": "Retrieves tokens from the CMS database with optional filtering, sorting, and pricing data",
				"tags": ["Tokens"],
				"parameters": [
					{
						"schema": {
							"type": "string",
							"enum": ["true", "false"],
							"description": "Filter by listed status"
						},
						"required": false,
						"name": "isListed",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"enum": ["underlyingToken", "pureSuperToken", "nativeAssetSuperToken", "wrapperSuperToken"],
							"description": "Type of token in the Superfluid ecosystem",
							"example": "wrapperSuperToken"
						},
						"required": false,
						"name": "tokenType",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"description": "Comma-separated list of tags to filter by",
							"example": "supertoken,testnet"
						},
						"required": false,
						"name": "tags",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"enum": ["true", "false"],
							"description": "Include current pricing data"
						},
						"required": false,
						"name": "includePricing",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"enum": ["name", "symbol", "chainId"],
							"description": "Field to sort by"
						},
						"required": false,
						"name": "sortBy",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"enum": ["asc", "desc"],
							"description": "Sort order"
						},
						"required": false,
						"name": "sortOrder",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"description": "Number of items per page",
							"example": "100"
						},
						"required": false,
						"name": "limit",
						"in": "query"
					},
					{
						"schema": {
							"type": "string",
							"description": "Page number",
							"example": "1"
						},
						"required": false,
						"name": "page",
						"in": "query"
					}
				],
				"responses": {
					"200": {
						"description": "Paginated list of tokens",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"tokens": {
											"type": "array",
